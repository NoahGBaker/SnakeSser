script/source = "extends CharacterBody2D

var Direction
const SPEED = 300.0

var GRID :Array

func _ready():
	for i in range(11):
		GRID.append([])
		for j in range(11):
			GRID[i].append(Vector2(i * 10, j * 10))
	print(GRID)

#Get place for snake to go along a certain sort of grid then set that goal to the previous snake body segment
func _physics_process(_delta):
	if Input.is_action_pressed(\"ui_left\"):
		Direction = \"left\"
		velocity.x = -1 * SPEED
		velocity.y = 0
		#$\".\".position = lerp(Vector2)
	if Input.is_action_pressed(\"ui_right\"):
		Direction = \"right\"
		velocity.x = 1 * SPEED
		velocity.y = 0
		#$\".\".position.y = move_toward($\".\".position.y,10,50)
	if Input.is_action_pressed(\"ui_down\"):
		Direction = \"down\"
		velocity.y = 1 * SPEED
		velocity.x = 0
		#$\".\".position.x = move_toward($\".\".position.x,10,50)
	if Input.is_action_pressed(\"ui_up\"):
		Direction = \"up\"
		velocity.y = -1 * SPEED
		velocity.x = 0
	
	move_and_slide()
"

